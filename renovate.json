{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "npm"
      ],
      "rangeStrategy": "pin"
    },
    {
      "groupName": "body-parser",
      "matchPackageNames": [
        "/@types/body-parser/",
        "/body-parser/"
      ]
    },
    {
      "groupName": "cors",
      "matchPackageNames": [
        "/@types/cors/",
        "/cors/"
      ]
    },
    {
      "groupName": "express",
      "matchPackageNames": [
        "/@types/express/",
        "/express/"
      ]
    },
    {
      "groupName": "node",
      "matchPackageNames": [
        "/@types/node/",
        "/node/"
      ]
    }
  ]
}
