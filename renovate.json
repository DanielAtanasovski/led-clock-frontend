{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchDatasources": ["npm"],
      "rangeStrategy": "pin"
    },
    {
      "packagePatterns": ["@types/body-parser", "body-parser"],
      "groupName": "body-parser"
    },
    {
      "packagePatterns": ["@types/cors", "cors"],
      "groupName": "cors"
    },
    {
      "packagePatterns": ["@types/express", "express"],
      "groupName": "express"
    },
    {
      "packagePatterns": ["@types/node", "node"],
      "groupName": "node"
    }
  ]
}
